N8N_WEBHOOK_URL=http://localhost:5678/webhook/contact-lead
N8N_WEBHOOK_SECRET=replace_with_shared_secret

# Landing page flags
# Master switch for advanced CRO features (true/false, on/off, 1/0)
NEXT_PUBLIC_CRO_MODE=true

# Enables URL intent-driven hero copy matching (?intent=ai|web|app)
NEXT_PUBLIC_ENABLE_INTENT_MATCHING=true

# Enables URL hero A/B override (?hero=control|creative or ?ab=a|b)
NEXT_PUBLIC_ENABLE_HERO_AB_QUERY=true

# Default hero visual when no query param is set: control or creative
NEXT_PUBLIC_HERO_VISUAL_DEFAULT=creative

# Shows mobile sticky CTA bar
NEXT_PUBLIC_SHOW_MOBILE_STICKY_CTA=true
